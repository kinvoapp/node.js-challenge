{
  "name": "advanced-node",
  "version": "3.0.0",
  "main": "index.js",
  "author": "gervasioartur <gervasioartur@outlook.com.br>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/main/server.js",
    "up": "sudo docker-compose up",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test -- ----watchAll",
    "test:staged": "yarn test -- --bail --findRelatedTests ./src",
    "test:coverage": "yarn test -- --coverage",
    "predebug": "typeorm migration:run",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9221 --nolazy ./dist/main/server.js",
    "test:integration": "yarn test -- --watch -c jest.integration.config.js",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config  --inspect=0.0.0.0:9223 src/main/server.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-mock": "^28.1.1",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^13.0.3",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.18",
    "pg-mem": "^1.9.17",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "brcypt": "^1.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "0.2.29",
    "validator": "^13.7.0"
  }
}
