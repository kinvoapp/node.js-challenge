  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "prisma migrate deploy && node dist/src/server.js",
    "dev": "npx prisma migrate dev && ts-node-dev -r dotenv/config src/server.ts",
    "test": "dotenv -e .env.test npx prisma migrate dev && jest --runInBand --coverage true",
    "test-unique": "dotenv -e .env.test npx prisma migrate dev && jest -t",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@prisma/client": "^4.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.3",
    "@types/uuid": "^8.3.4",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "faker": "^6.6.6",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^3.2.6",
    "prisma": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^6.0.0",
    "jest": "^28.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5"
  }
