generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum TransactionType {
  Deposit
  Withdraw
}

model Transaction {
  id          String          @id @default(uuid())
  value       String
  type        TransactionType
  description String?

  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt

  @@map("transactions")
}
